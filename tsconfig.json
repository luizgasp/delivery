{
  "compilerOptions": {
    "target": "es2021",
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": ".",
    "module": "commonjs",
    "strict": true,
    "lib": [
      "esnext"
    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}